# MalwareBazaar
MB_API_KEY=
MB_ZIP_PASSWORD=infected
# Some samples contain nested archives; try this password for nested zip/7z/rar extraction.
MB_NESTED_PASSWORD=infected

# Output dirs (in Docker, recommend /data/*)
MB_DOWNLOAD_DIR=/data/download
MB_UNZIP_DIR=/data/unzip

# Rate limiting / retries (avoid connection drops / throttling)
MB_SLEEP_MIN=2
MB_SLEEP_MAX=6
MB_RETRY_MAX=8
MB_RETRY_BASE_SLEEP=2
MB_RETRY_MAX_SLEEP=60
MB_TIMEOUT=180

# Hybrid-Analysis (browser cookie)
HA_COOKIE=
HA_SLEEP_MIN=2
HA_SLEEP_MAX=6
HA_RETRY_MAX=5
HA_CONNECT_TIMEOUT=10
HA_MAX_TIME=60


